{
  "name": "my-new-project",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "test": "node ./node_modules/jest/bin/jest.js",
    "lint": "yarn run lint:js",
    "lint-fix": "yarn run lint-fix:js",
    "lint:js": "eslint .",
    "lint-fix:js": "eslint . --fix",
    "build": "expo build:android"
  },
  "jest": {
    "preset": "jest-expo",
    "snapshotSerializers": ["enzyme-to-json/serializer"],
    "transformIgnorePatterns": [
      "node_modules/(?!native-base-shoutem-theme|expo-react-native-adapter|react-native|expo|@expo|react-navigation-stack|react-navigation|native-base|babel-runtime|enzyme)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/native-base-theme/**",
      "!**/coverage/**"
    ]
  },
  "dependencies": {
    "@expo/samples": "2.1.1",
    "axios": "^0.18.0",
    "babel-core": "",
    "babel-eslint": "^8.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-react-native": "^3.5.0",
    "expo": "^30.0.1",
    "native-base": "^2.8.1",
    "prop-types": "^15.6.2",
    "react": "16.3.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-30.0.0.tar.gz",
    "react-native-dash": "^0.0.9",
    "react-native-drawer": "^2.5.1",
    "react-native-responsive-screen": "^1.1.10",
    "react-native-sidebar": "^0.3.0",
    "react-native-typography": "^1.4.0",
    "react-navigation": "^2.16.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.1",
    "redux-axios-middleware": "^4.0.0",
    "rn-nested-stylesheet": "^1.0.2"
  },
  "devDependencies": {
    "@babel/runtime": "7.0.0-beta.55",
    "classnames": "^2.2.6",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "expo-cli": "^2.2.4",
    "jest-expo": "30.0.0",
    "npx": "^10.2.0",
    "react-dom": "^16.6.0",
    "react-native-dotenv": "^0.2.0",
    "redux-mock-store": "^1.5.3"
  }
}
